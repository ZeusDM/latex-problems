for i in {1959..2019};
do
    mkdir $i
    rm $i/source_pt.tex
    rm $i/source_en.tex
    rm $i/source.tex
    echo "IMO $i" >> $i/source_pt.tex
    echo "IMO $i" >> $i/source_en.tex
	for j in {1..6}
	do
		mkdir $i/$j
		rm $i/$j/source_pt.tex
		rm $i/$j/source_en.tex
		rm $i/$j/number.tex
		echo "$j" >> $i/$j/number.tex
	done
done
